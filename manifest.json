{
  "name": "Password Generator",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Extension to generate passwords",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "web_accessible_resources": [
    {
      "matches": ["<all_urls>"],
      "resources": ["icon.png"]
    }
  ]
}
